<Window x:Class="WpfApplication1.InsegnanteManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Gestionale Insegnanti " Height="300" Width="300" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <DockPanel LastChildFill="False" Margin="0,0,0, 0">
        <DockPanel.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFEDF7BC"/>
                <GradientStop Color="#FF85BD67" Offset="1"/>
            </LinearGradientBrush>
        </DockPanel.Background>
    </DockPanel>
</Window>
